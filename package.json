{
  "name": "ac-ratelimiter",
  "description": "Simple ratelimiter for express",
  "version": "1.0.1",
  "author": "Mark Poepping / support@admiralcloud.com",
  "repository": {
    "type": "git",
    "url": "git://github.com/mmpro/ac-ratelimiter"
  },
  "homepage": "https://www.admiralcloud.com",
  "dependencies": {
    "ac-ip": "^1.2.0",
    "async": "^3.1.0",
    "lodash": "^4.17.15"
  },
  "devDependencies": {
    "ac-conventional-changelog-template": "git+https://github.com/mmpro/ac-conventional-changelog-template.git",
    "corp-semantic-release": "git+https://github.com/mmpro/corp-semantic-release.git",
    "cz-customizable": "git+https://github.com/mmpro/cz-customizable.git",
    "eslint": "6.x",
    "expect": "^24.9.0",
    "mocha": "^6.2.0",
    "mocha-jenkins-reporter": "^0.4.1",
    "mocha-junit-reporter": "^1.18.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": ".czConfig.js"
    }
  },
  "scripts": {
    "test": "./node_modules/.bin/mocha --slow 1000 --colors ./test/test.js || :",
    "test-local": "env TESTMODE=true ./node_modules/.bin/mocha --slow 1000 --colors ./test/test.js || :",
    "test-jenkins": "JUNIT_REPORT_PATH=./report.xml mocha --slow 1000 --colors --reporter mocha-jenkins-reporter --reporter-options junit_report_name='RATELIMITER'"
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
